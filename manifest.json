{
    "manifest_version": 3,
    "name": "TimeAware",
    "description": "Manage your screentime, click the extension to set your preferences.",
    "version": "1.4",
    "action": {
      "default_popup": "main.html",
      "default_icon": "images/time-aware-logo-no-background.png"
    },
    "permissions": [
    "storage"
    ],
      "content_scripts": [
         {
            "matches": ["<all_urls>"],
            "js": ["content-script.js"]
         }
         ]
  }